<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<TransformGroup x:Key="RotateFlipX">
		<ScaleTransform ScaleY="1" ScaleX="-1"/>
		<SkewTransform AngleY="0" AngleX="0"/>
		<RotateTransform Angle="0"/>
		<TranslateTransform/>
	</TransformGroup>
	<DropShadowEffect x:Key="TextDropShadow" Opacity="0.5"/>
	<DropShadowEffect x:Key="WindowDropShadow" Direction="270" BlurRadius="30" RenderingBias="Quality" Opacity="0.6" ShadowDepth="10"/>
	<DropShadowEffect x:Key="DropShadowAround" ShadowDepth="0" Opacity="0.8" BlurRadius="8"/>
	<!-- Resource dictionary entries should be defined here. -->
</ResourceDictionary>